{
"update_url": "https://clients2.google.com/service/update2/crx",

  "description": "Selenium IDE alternative to record and export Selenium scripts. With reports & screenshots. Fast & open-source.",
  "manifest_version": 3,
  "name": "Testoriumz Recorder (Selenium tests generator)",
  "version": "7.1.0",
  "homepage_url": "https://reporting.linkfields.com/ui/#login",
  "segment_url": "https://reporting.linkfields.com/api/v1/lfi_automation_suite/dashboard?page.page=1&page.size=50&page.sort=creationDate%2CDESC",
  "hubspot_url": "https://reporting.linkfields.com/ui/#login",
  "icons": {
    "16": "katalon/images/branding/branding_16.png",
    "48": "katalon/images/branding/branding_48.png",
    "128": "katalon/images/branding/branding_128.png"
  },
  "applications": {
    "gecko": {
      "id": "{91f05833-bab1-4fb1-b9e4-187091a4d75d}"
    }
  },
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus",
    "downloads",
    "webNavigation",
    "notifications",
    "cookies",
    "storage",
    "unlimitedStorage",
    "debugger",
    "scripting",
    "offscreen"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/",
    "<all_urls>"
  ],
  "externally_connectable": {
    "ids": [
      "*"
    ],
    "matches": [
      "https://developer.mozilla.org/*",
      "https://reporting.linkfields.com/*"
    ],
    "accepts_tls_channel_id": false
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; unsafe-eval; unsafe-inline;"
  },
  "sandbox": {
    "pages": [
      "panel/sandbox.html"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "common/browser-polyfill-content.js",
        "common/remote-object-helper-content.js",
        "common/chrome-polyfill-server.js"
      ],
      "match_about_blank": true,
      "all_frames": true,
      "run_at": "document_start"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "match_about_blank": true,
      "all_frames": true,
      "run_at": "document_start",
      "world": "MAIN",
      "js": [
        "common/remote-object-helper-page.js"
      ]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "match_about_blank": true,
      "all_frames": true,
      "run_at": "document_start",
      "world": "MAIN",
      "js": [
        "bundles/content.1.bundle.js"
      ]
    },
    {
      "matches": [
        "https://www.facebook.com/dialog/share?*",
        "https://twitter.com/intent/tweet?*",
        "https://www.linkedin.com/sharing/share-offsite/?*"
      ],
      "js": [
        "bundles/content.2.bundle.js"
      ],
      "match_about_blank": true,
      "all_frames": true,
      "world": "MAIN"
    },
    {
      "matches": [
        "https://reporting.linkfields.com/*"
      ],
      "js": [
        "content/portal-auth-extractor.js"
      ],
      "match_about_blank": true,
      "all_frames": false,
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "worker_wrapper.js"
  },
  "action": {
    "default_icon": "katalon/images/branding/branding_16.png",
    "default_title": "Testoriumz Recorder"
  },
  "default_popup": "popup-browser/index.html",
  "web_accessible_resources": [
    {
      "resources": [
        "page/prompt.js",
        "page/runScript.js",
        "katalon/authenticated.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "options_page": "katalon/options.html",
  "offline_enabled": true
}